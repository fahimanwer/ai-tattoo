{
  "expo": {
    "name": "AI Tattoo",
    "slug": "ai-tattoo",
    "version": "1.1.5",
    "orientation": "portrait",
    "scheme": "aitattoo",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "dev.codewithbeto.aitattoo",
      "icon": "./assets/AiTattooIcon.icon",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "fb",
          "instagram",
          "twitter",
          "tiktoksharesdk"
        ]
      },
      "appleTeamId": "T2A8YY9YDW",
      "entitlements": {
        "com.apple.security.application-groups": ["group.codewithbeto.aitattoo"]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "dev.codewithbeto.aitattoo",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to let you select a tattoo to try on."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take a photo of your arm, leg, or face to try on a tattoo.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone to take a photo of your arm, leg, or face to try on a tattoo.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to select a photo to try on a tattoo.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to select a photo to try on a tattoo.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-share",
        {
          "ios": [
            "fb",
            "instagram",
            "twitter",
            "tiktoksharesdk",
            "fb",
            "instagram",
            "twitter",
            "tiktoksharesdk"
          ],
          "android": [
            "com.facebook.katana",
            "com.instagram.android",
            "com.twitter.android",
            "com.zhiliaoapp.musically",
            "com.facebook.katana",
            "com.instagram.android",
            "com.twitter.android",
            "com.zhiliaoapp.musically"
          ]
        }
      ],
      "expo-apple-authentication",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      "expo-asset",
      "expo-sqlite",
      "expo-image",
      "expo-video",
      "@bacons/apple-targets",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ab9df978-7734-423b-a42f-e0028dc1b749"
      }
    },
    "owner": "betoatexpo",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ab9df978-7734-423b-a42f-e0028dc1b749"
    }
  }
}
