{
  "expo": {
    "name": "AI Tattoo",
    "slug": "ai-tattoo",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/ios-tinted.png",
    "scheme": "aitattoo",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "dev.codewithbeto.aitattoo",
      "icon": "./assets/AiTattooIcon.icon",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1",
      "appleTeamId": "T2A8YY9YDW"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "dev.codewithbeto.aitattoo"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to let you select a tattoo to try on."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take a photo of your arm, leg, or face to try on a tattoo.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone to take a photo of your arm, leg, or face to try on a tattoo.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to select a photo to try on a tattoo.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to select a photo to try on a tattoo.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-share",
        {
          "ios": ["fb", "instagram", "twitter", "tiktoksharesdk"],
          "android": [
            "com.facebook.katana",
            "com.instagram.android",
            "com.twitter.android",
            "com.zhiliaoapp.musically"
          ]
        }
      ],
      "expo-apple-authentication",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      "expo-asset",
      "expo-sqlite",
      "expo-build-properties",
      "expo-image",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ab9df978-7734-423b-a42f-e0028dc1b749"
      }
    },
    "owner": "betoatexpo"
  }
}
